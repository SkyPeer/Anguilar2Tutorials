{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": false,
        "pretty": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "types": []
    },
    "files": [

        "build/compiled/main.ts",
        "./typings/index.d.ts"
    ],
    "exclude": [
        "node_modules"
    ],
    "angularCompilerOptions": {

        "skipMetadataEmit": true
    }
}